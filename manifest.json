{
    "manifest_version": 2,
    "name": "Immutable小工具",
    "version": "1.0",
    "description": "对象.log 即可查看美化后的结果",
    "author": "panwz",
    "icons":
    {
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
    }],
    "web_accessible_resources": [
        "inject.js"
    ],
    "permissions": [
        "<all_urls>"
    ]
}